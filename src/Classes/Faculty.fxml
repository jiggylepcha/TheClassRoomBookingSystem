<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Classes.FacultyController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="logOutButton" layoutX="574.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#clickLogOutButton" text="Logout" />
            <Button layoutX="448.0" layoutY="173.0" mnemonicParsing="false" text="View Rooms" fx:id="viewRoom" onMouseClicked="#clickRoomAvailibiity"/>
            <Button layoutX="92.0" layoutY="173.0" mnemonicParsing="false" text="Book Room" fx:id="bookRoom" onMouseClicked="#clickBookRoom"/>
            <Button layoutX="261.0" layoutY="173.0" mnemonicParsing="false" text="Cancel Booking" fx:id="cancelBooking" onMouseClicked="#clickUnbookRoom"/>
            <Label layoutX="30.0" layoutY="19.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
