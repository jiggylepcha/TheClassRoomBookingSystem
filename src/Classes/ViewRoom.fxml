<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Classes.DialogController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button layoutX="359.0" layoutY="334.0" mnemonicParsing="false" onMouseClicked="#clickCheckAvailability" text="Check Availability" />
            <Button fx:id="cancelButton" layoutX="544.0" layoutY="334.0" mnemonicParsing="false" onMouseClicked="#clickCancelButton" text="Cancel" />
            <Label layoutX="129.0" layoutY="88.0" text="Room Number" />
            <Label layoutX="128.0" layoutY="153.0" text="Time" />
            <TextField fx:id="roomNumber" layoutX="284.0" layoutY="83.0" />
            <TextField fx:id="time" layoutX="284.0" layoutY="149.0" />
            <TextField layoutX="284.0" layoutY="217.0" fx:id="day"/>
            <Label layoutX="132.0" layoutY="222.0" text="Day" />
            <Label layoutX="267.0" layoutY="23.0" text="View Room" />
         </children>
        </AnchorPane>
    </children>
</VBox>
