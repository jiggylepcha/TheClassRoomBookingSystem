<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<VBox prefHeight="1000.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Classes.Controller">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Button layoutX="331.0" layoutY="377.0" mnemonicParsing="false" text="LogIn"  fx:id="loginButton" onMouseClicked="#clickLogIn"/>
                <TextFlow layoutX="275.0" layoutY="72.0" prefHeight="25.0" prefWidth="157.0" />
                <Label alignment="CENTER" layoutX="104.0" layoutY="221.0" prefHeight="27.0" prefWidth="171.0" text="Username" textAlignment="CENTER" />
                <Label alignment="CENTER" layoutX="104.0" layoutY="440.0" prefHeight="41.0" prefWidth="157.0" text="Don't Have An Account ?" textAlignment="CENTER" />
                <Button fx:id="signUpButton" accessibleRole="HYPERLINK" layoutX="303.0" layoutY="447.0" mnemonicParsing="false" onMouseClicked="#clickSignUp" prefHeight="25.0" prefWidth="103.0" text="Sign Up" />
                  <TextField layoutX="265.0" layoutY="219.0" fx:id="userID" />
            <Label layoutX="158.0" layoutY="307.0" text="Password" />
            <PasswordField fx:id="passwordField" layoutX="265.0" layoutY="294.0" onMouseClicked="#clickLogIn"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
